{
  "servers": [
    {
      "port": "${port1}",
      "host": "${url1}",
      "alias": "java-consumer",
      "queries": [
        {
          "outputWriters": [
            {
              "@class": "com.googlecode.jmxtrans.model.output.InfluxDbWriterFactory",
              "url": "${influxUrl}",
              "database": "${influxDb}",
              "username": "${influxUser}",
              "password": "${influxPwd}"
            }
          ],
          "obj": "java.lang:type=Memory",
          "attr": [
            "HeapMemoryUsage",
            "NonHeapMemoryUsage"
          ],
          "resultAlias": "jvmMemory"
        },
        {
          "outputWriters": [
            {
              "@class": "com.googlecode.jmxtrans.model.output.InfluxDbWriterFactory",
              "url": "${influxUrl}",
              "database": "${influxDb}",
              "username": "${influxUser}",
              "password": "${influxPwd}"
            }
          ],
          "obj": "java.lang:type=OperatingSystem",
          "attr": [
            "FreePhysicalMemorySize",
            "ProcessCpuLoad",
            "SystemCpuLoad",
            "SystemLoadAverage"
          ],
          "resultAlias": "os"
        },
        {
          "outputWriters": [
            {
              "@class": "com.googlecode.jmxtrans.model.output.InfluxDbWriterFactory",
              "url": "${influxUrl}",
              "database": "${influxDb}",
              "username": "${influxUser}",
              "password": "${influxPwd}"
            }
          ],
          "obj": "kafka.consumer:type=consumer-fetch-manager-metrics,client-id=*",
          "attr": [
            "bytes-consumed-rate",
            "fetch-latency-avg",
            "fetch-rate", 
            "records-lag-max",
            "records-per-request-avg",
            "records-consumed-rate"
          ],
          "resultAlias": "ConsumerMetrics"
        }
      ],
      "numQueryThreads": 2
    }
  ]
}
